<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_nav"
    app:startDestination="@id/main_page">

    <fragment
        android:id="@+id/main_page"
        android:name="kr.khs.studyfarm.mainpage.MainFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/action_bottom_home_to_notificationFragment"
            app:destination="@id/notificationFragment" />
        <action
            android:id="@+id/action_bottom_home_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_main_page_to_makeStudyFragment"
            app:destination="@id/makeStudyFragment" />
    </fragment>
    <fragment
        android:id="@+id/study_list"
        android:name="kr.khs.studyfarm.mainpage.study_list.StudyListFragment"
        android:label="fragment_study_list"
        tools:layout="@layout/fragment_study_list" />
    <fragment
        android:id="@+id/bottom_more"
        android:name="kr.khs.studyfarm.Blank2Fragment"
        android:label="fragment_blank2"
        tools:layout="@layout/fragment_blank2" />
    <fragment
        android:id="@+id/notificationFragment"
        android:name="kr.khs.studyfarm.noti_page.NotificationFragment"
        android:label="NotificationFragment" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="kr.khs.studyfarm.setting.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_profileFragment_to_selectFragment2"
            app:destination="@id/selectFragment2" />
    </fragment>
    <fragment
        android:id="@+id/selectFragment2"
        android:name="kr.khs.studyfarm.login_process.select.SelectFragment"
        android:label="SelectFragment" />
    <fragment
        android:id="@+id/makeStudyFragment"
        android:name="kr.khs.studyfarm.mainpage.make_study.MakeStudyFragment"
        android:label="MakeStudyFragment" >
        <action
            android:id="@+id/action_makeStudyFragment_to_main_page"
            app:destination="@id/main_page"
            app:popUpTo="@id/main_page" />
        <argument
            android:name="topics"
            app:argType="kr.khs.studyfarm.network.response.UserInterestingInfo[]" />
        <argument
            android:name="areas"
            app:argType="kr.khs.studyfarm.network.response.UserCityInfo[]" />
        <action
            android:id="@+id/action_makeStudyFragment_to_findSCFragment"
            app:destination="@id/findSCFragment" />
    </fragment>
    <fragment
        android:id="@+id/findSCFragment"
        android:name="kr.khs.studyfarm.mainpage.make_study.FindSCFragment"
        android:label="FindSCFragment" >
        <action
            android:id="@+id/action_findSCFragment_to_makeStudyFragment"
            app:destination="@id/makeStudyFragment" />
    </fragment>
</navigation>